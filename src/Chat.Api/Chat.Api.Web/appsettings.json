    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "logs/log-{Date}.txt",
          "formatter": "Serilog.Formatting.Json.JsonFormatter",
          "rollingInterval": "Day",
          "retainedFileCountLimit": null,
          "fileSizeLimitBytes": null
        }
      }
    ],
    "MinimumLevel": {
